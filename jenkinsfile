pipeline
{
        agent any
        stages
          {
              
              stage("Continous-Downloading git")
              {
                  
                  steps
                  {
                      
                     git 'https://github.com/ashvinGi/mymaven.git' 
                      
                  }
                  
              }
              stage("Continous-Build git")
              {
                  
                  steps
                  {
                      
                  sh 'mvn package' 
                      
                  }
                  
              }
              
              
              
          }
                
        
        
        
        
        
        
        
    
    
    
    
    
    
}
